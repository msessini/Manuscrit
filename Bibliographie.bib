@article{LHCCP4,
title = {Determination of the Higgs CP-mixing angle in the tau decay channels},
journal = {Nuclear and Particle Physics Proceedings},
volume = {273-275},
pages = {841-845},
year = {2016},
note = {37th International Conference on High Energy Physics (ICHEP)},
issn = {2405-6014},
doi = {https://doi.org/10.1016/j.nuclphysbps.2015.09.129},
url = {https://www.sciencedirect.com/science/article/pii/S2405601415006185},
author = {Stefan Berge and Werner Bernreuther and Sebastian Kirchner},
keywords = {tau leptons, Z boson, spin correlations, parity, -violation},
abstract = {The measurement of possible Higgs sector CP-violation in the tau decay channels at the LHC is investigated. A CP-violating effect would manifest itself in these decay modes in characteristic spin-spin correlations of the tau lepton pairs which can be accessed using the momenta and impact parameters of the charged tau decay particles. We examine a CP-sensitive observable for a 125 GeV Higgs boson resonance in the gluon fusion channel at the LHC. Furthermore, we consider the distribution of this observable for the irreducible Drell-Yan background. By splitting these events into two categories we obtain two different distributions which can be used for calibration purposes. Finally, we estimate the achievable precision of the scalar-pseudo-scalar mixing angle of the tau decay channel for Run II and the high luminosity run of the LHC.}
}

